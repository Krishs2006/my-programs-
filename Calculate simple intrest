#include <stdio.h>

int main() {
    float principal, rate, time;

    printf("Enter principal: ");
    scanf("%f", &principal);

    printf("Enter rate: ");
    scanf("%f", &rate);

    printf("Enter time: ");
    scanf("%f", &time);

    float interest = (principal * rate * time) / 100;

    printf("Interest: %f\n", interest);

    return 0;
}
