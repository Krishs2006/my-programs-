#include <stdio.h>

int main() {
    // 1D array
    int arr1[5] = {1, 2, 3, 4, 6};
    printf("1D array: ");
    for (int i = 0; i < 5; i++)
        printf("%d ", arr1[i]);
    printf("\n");

    // 2D array
    int arr2[3][3] = {{1, 2, 3}, {4, 5, 6}, {7, 8, 9}};
    printf("2D array:\n");
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++)
            printf("%d ", arr2[i][j]);
        printf("\n");
    }

    return 0;
}
